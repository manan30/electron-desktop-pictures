{
  "root": true,
  "env": {
    "es2020": true,
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks", "prettier"],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "react/jsx-filename-extension": "off"
  }
}
